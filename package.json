{
  "name": "triplit-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier src/ --write",
    "db:dev": "npx triplit dev",
    "db:snap": "triplit snapshot create --outDir ./triplit/snapshots",
    "db:seed": "triplit snapshot push --snapshot ./triplit/snapshots",
    "db:push": "triplit schema push",
    "email": "cp .env ./node_modules/react-email/dist/preview && email dev --dir src/components/emails",
    "up:check": "pnpx npm-check-updates",
    "up:go": "ncu -u && pnpm install",
    "stripe": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "stripe2": "stripe listen --forward-to localhost:3000/api/auth/stripe/webhook"
  },
